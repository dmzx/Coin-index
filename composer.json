	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2019-01-03",
	"keywords": [
		"phpbb",
		"extension",
		"coinindex"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
